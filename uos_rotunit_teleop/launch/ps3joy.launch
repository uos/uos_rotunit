<?xml version="1.0"?>
<launch>
	<node pkg="ps3joy" type="ps3joy.py" name="ps3joy" output="screen" launch-prefix="sudo"/>

	<node pkg="joy" type="joy_node" name="joy_node">
		<param name="autorepeat_rate" value="10"/>
		<param name="dev" value="/dev/input/js0"/>
	</node>
  
  <node name="uos_rotunit_teleop_ps3joy" pkg="uos_rotunit_teleop" type="uos_rotunit_teleop_ps3joy" output="screen" >
	  <param name="acc" type="double" value="0.01" />
  </node>
</launch>
